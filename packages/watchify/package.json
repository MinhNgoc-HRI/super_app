{
  "name": "watchify",
  "version": "0.0.1",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "start": "react-native webpack-start --port 9000 --reset-cache",
    "start:standalone": "STANDALONE=1 react-native webpack-start --port 8081 --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc",
    "bundle:ios": "react-native webpack-bundle --platform ios --entry-file index.js --dev false",
    "bundle:android": "react-native webpack-bundle --platform android --entry-file index.js --dev false",
    "pods": "(cd ios && pod install)"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^18.7.1",
    "@react-native-firebase/app": "^18.7.1",
    "@react-native-firebase/crashlytics": "^18.7.1",
    "@react-native-firebase/messaging": "^18.7.1",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/bottom-tabs": "next",
    "@react-navigation/drawer": "next",
    "@react-navigation/material-top-tabs": "next",
    "@react-navigation/native": "next",
    "@react-navigation/native-stack": "next",
    "@react-navigation/stack": "next",
    "@shopify/react-native-skia": "^0.1.225",
    "@shopify/restyle": "^2.4.2",
    "lottie-react-native": "^6.4.1",
    "pmn-rn-component": "0.1.3",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-circular-progress-indicator": "4.4.2",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "2.14.0",
    "react-native-pager-view": "^6.2.3",
    "react-native-reanimated": "3.6.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^14.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@callstack/repack": "^3.6.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "4.8.4",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">=16"
  }
}
